{
  "name": "frontend",
  "version": "1.0.0",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "npm run dev-standard && npm run dev-suite",
    "dev-standard": "rimraf dist && npm run build-standard && npm run copy-to-work",
    "build-standard": "npm run clean-work-folder && rimraf dist && rollup -c --environment BUILD:production --environment BUILDTYPE:standard",
    "build-suite": "npm run clean-design-folder && rimraf dist-training && rollup -c --environment BUILD:production --environment BUILDTYPE:suite",
    "create-folders": "mkdirp ./dist && mkdirp ../src/main/resources/static/ext",
    "clean-work-folder": "rimraf ../training-handson-work/src/main/resources/static/ext && mkdirp ../training-handson-work/src/main/resources/static/ext/",
    "clean-design-folder": "rimraf ../training-handson-design/src/main/resources/public/js/flowable-forms-training/ && mkdirp ../training-handson-design/src/main/resources/public/js/flowable-forms-training/",
    "dev-suite": "rimraf dist-suite && npm run build-suite && npm run copy-to-design",
    "copy-to-work": "cpx ./dist/**/* ../training-handson-work/src/main/resources/static/ext",
    "copy-to-design": "cpx  ./dist-suite/**/* ../training-handson-design/src/main/resources/public/js/flowable-forms-training/"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@babel/preset-typescript": "7.23.2",
    "@flowable/forms": "3.13.10",
    "@flowable/work-scripts": "3.13.10",
    "@rollup/plugin-replace": "5.0.2",
    "@rollup/plugin-typescript": "11.1.1",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addon-storysource": "6.5.16",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/react": "6.5.16",
    "@storybook/testing-library": "0.0.13",
    "babel-loader": "9.1.2",
    "concurrently": "7.6.0",
    "cpr": "3.0.1",
    "cpx": "1.5.0",
    "rollup": "3.18.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-scss": "4.0.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "sass": "1.58.3",
    "sass-loader": "13.2.0",
    "storybook-addon-mock": "3.2.0",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.13",
    "@mui/x-date-pickers": "6.16.2",
    "pigeon-maps": "0.21.3",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "compilerOptions": {
    "jsx": "react",
    "module": "esNext",
    "moduleResolution": "node",
    "outDir": "./dist",
    "preserveConstEnums": true,
    "strict": true,
    "strictFunctionTypes": false,
    "target": "es5",
    "sourceMap": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}
